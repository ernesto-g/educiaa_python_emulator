<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="window1">
    <child>
      <object class="GtkFixed" id="fixed1">
        <property name="visible">True</property>
        <child>
          <object class="GtkScrolledWindow" id="scrolledwindow1">
            <property name="width_request">680</property>
            <property name="height_request">320</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="hscrollbar_policy">automatic</property>
            <property name="vscrollbar_policy">automatic</property>
            <child>
              <object class="GtkTextView" id="txtConsole">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="y">44</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btnSend">
            <property name="label" translatable="yes">Send</property>
            <property name="width_request">50</property>
            <property name="height_request">32</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="y">370</property>
          </packing>
        </child>
        <child>
          <object class="GtkEntry" id="txtTosend">
            <property name="width_request">615</property>
            <property name="height_request">32</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="invisible_char">&#x25CF;</property>
          </object>
          <packing>
            <property name="x">65</property>
            <property name="y">370</property>
          </packing>
        </child>
        <child>
          <object class="GtkRadioButton" id="rbtnAscii">
            <property name="label" translatable="yes">ASCII</property>
            <property name="width_request">75</property>
            <property name="height_request">32</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">False</property>
            <property name="active">True</property>
            <property name="draw_indicator">True</property>
          </object>
          <packing>
            <property name="x">550</property>
          </packing>
        </child>
        <child>
          <object class="GtkRadioButton" id="rbtnHex">
            <property name="label" translatable="yes">HEX</property>
            <property name="width_request">50</property>
            <property name="height_request">32</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">False</property>
            <property name="active">True</property>
            <property name="draw_indicator">True</property>
          </object>
          <packing>
            <property name="x">630</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
